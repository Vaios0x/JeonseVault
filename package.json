  "bugs": {
    "url": "https://github.com/jeonsevault/jeonsevault/issues"
  },
  "homepage": "https://jeonsevault.com",
  "funding": {
    "type": "individual",
    "url": "https://jeonsevault.com/donate"
  }
}
